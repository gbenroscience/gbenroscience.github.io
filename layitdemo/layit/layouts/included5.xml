<ConstraintLayout
        width="90px"
        height="match_parent"
        cx_align="parent"
>

    <Label
        id="block_title_5"
        width="wrap_content"
        height="wrap_content"
        cx_align='parent'
        top_top="parent"
        marginTop="16"
        textColor="black"
        value="Even more details"
        textSize="1.2em"
        textStyle="bold"
        fontFamily="Arial"
    />
    
        
    
    <TextField
        id="phone_input_5"
        width="150px"
        height="32px"
        start_start="parent"
        end_end="parent"
        top_bottom="block_title_5"
        margin="8"
        placeholder="Church Line"
        background="white"
        fontFamily="Arial"
        padding="8px"
        inputType="phone"
        text=""
        value=""
        textColor="black"
        textSize="1.2em"
        textStyle="bold"
        borderRadius="10px"
    />

    <include
            layout="included6"
            id="next_level_details"
            width="250"
            height="120"
            marginStart="0px"
            cx_align="parent"
            top_bottom="phone_input_5"
            border="0px solid gray"
            borderRadius="0em"
    />

    
    
</ConstraintLayout>