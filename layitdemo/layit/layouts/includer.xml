<ConstraintLayout
        width="match_parent"
        height="match_parent">

    <include
            layout="included1"
            id="basic_details"
            width="18%"
            height="220"
            start_start="parent"
            cy_align="parent"
            border="1px solid darkred"
            borderRadius="1em"
    />


    <include
            layout="included2"
            id="more_details"
            width="18%"
            height="220"
            marginStart="32px"
            start_end="basic_details"
            cy_align="basic_details"
            border="1px solid black"
            borderRadius="1em"
    />

    <include
            layout="included3"
            id="yet_more_details"
            width="18%"
            height="220"
            marginStart="32px"
            start_end="more_details"
            cy_align="more_details"
            border="1px solid black"
            borderRadius="1em"
    />
    <include
            layout="included4"
            id="leisure_details"
            width="18%"
            height="220"
            marginTop="0px"
            marginStart="32px"
            start_end="yet_more_details"
            cy_align="yet_more_details"
            border="1px solid black"
            borderRadius="1em"
    />

    <include
            layout="included5"
            id="church_details"
            width="18%"
            height="220"
            marginTop="0px"
            marginStart="32px"
            cx_align="leisure_details"
            top_top="parent"
            start_end="leisure_details"
            border="1px solid black"
            borderRadius="1em"
    />

</ConstraintLayout>